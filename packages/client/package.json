		"@tailwindcss/vite": "^4.1.14",
		"bits-ui": "^2.11.4",
		"daisyui": "^5.1.27",
		"heroicons-svelte": "^2.0.2",
		"svelte": "^5.39.9",
		"svelte-check": "^4.3.2",
		"tailwindcss": "^4.1.14",
		"vite": "^7.1.9",
		"wrangler": "^4.42.0"
